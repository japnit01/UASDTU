                    <img src="/curr-members/<%= m.img %>" height="120" width="120" style="border-radius: 50%; border: 5px solid #ffffff;">
                    <p style="font-size: 18px; font-weight: bold; text-transform: uppercase; margin-bottom: 10px;"><%= m.name %></p>
                    <div>
                        <% if(m.lkdn) { %>
                        <a href="<%= m.lkdn %>" target="blank"><i class="fab fa-linkedin"></i></i></a>                
                        <% } %>
                        <% if(m.mail) { %>    
                        <a href="mailto:<%= m.mail %>" target="blank"><i class="fab fa-google"></i></i></a>
                        <% } %>
                    </div>
                </div>
            </div>
            <% }) %>
    </div>

    
    <h3 class="center">Airframe Technicians</h3>

    <div class="outer">
        <% members["airframe"].map(m => { %>
            <div class="box1" style="width: 20%; min-width: 180px; padding: 0 20px;">
                <div class="ctn" style="transform: translateY(-20%);">
                    <img src="/curr-members/<%= m.img %>" height="120" width="120" style="border-radius: 50%; border: 5px solid #ffffff;">
                    <p style="font-size: 18px; font-weight: bold; text-transform: uppercase; margin-bottom: 10px;"><%= m.name %></p>
                    <div>
                        <% if(m.lkdn) { %>
                        <a href="<%= m.lkdn %>" target="blank"><i class="fab fa-linkedin"></i></i></a>                
                        <% } %>
                        <% if(m.mail) { %>    
                        <a href="mailto:<%= m.mail %>" target="blank"><i class="fab fa-google"></i></i></a>
                        <% } %>
                    </div>
                </div>
            </div>
            <% }) %>
    </div>

    
    <h3 class="center">Corporate</h3>

    <div class="outer">
        <% members["corporate"].map(m => { %>
            <div class="box1" style="width: 20%; min-width: 180px; padding: 0 20px;">
                <div class="ctn" style="transform: translateY(-20%);">
                    <img src="/curr-members/<%= m.img %>" height="120" width="120" style="border-radius: 50%; border: 5px solid #ffffff;">
                    <p style="font-size: 18px; font-weight: bold; text-transform: uppercase; margin-bottom: 10px;"><%= m.name %></p>
                    <div>
                        <% if(m.lkdn) { %>
                        <a href="<%= m.lkdn %>" target="blank"><i class="fab fa-linkedin"></i></i></a>                
                        <% } %>
                        <% if(m.mail) { %>    
                        <a href="mailto:<%= m.mail %>" target="blank"><i class="fab fa-google"></i></i></a>
                        <% } %>
                    </div>
                </div>
            </div>
            <% }) %>
    </div>
</div>

<script>
    let main = document.querySelector('.main');
    let nav = document.querySelector('.transparent');

    let shift = window.getComputedStyle(nav).getPropertyValue('height');
    // console.log(shift);
    main.style.marginTop = shift;
    document.title = "Current Members â€¢ UAS-DTU";
</script>

<%-include("partials/footer")%>
